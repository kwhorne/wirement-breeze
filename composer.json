{
    "name": "kwhorne/wirement-breeze",
    "description": "A custom package for Filament with login flow, profile and teams support.",
    "keywords": [
        "kwhorne",
        "laravel",
        "wirement-breeze"
    ],
    "homepage": "https://github.com/kwhorne/wirement-breeze",
    "license": "MIT",
    "authors": [
        {
            "name": "Knut W. Horne",
            "email": "kh@gets.no",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "bacon/bacon-qr-code": "^3.0",
        "filament/filament": "^4.0",
        "illuminate/contracts": "^11.0|^12.0",
        "jenssegers/agent": "^2.6",
        "pragmarx/google2fa": "^8.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.20|^3.7",
        "pestphp/pest-plugin-arch": "^2.0|^v3.0.0",
        "pestphp/pest-plugin-laravel": "^2.0|^v3.1.0",
        "pestphp/pest-plugin-livewire": "^2.1|^v3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Kwhorne\\WirementBreezeee\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kwhorne\\WirementBreezeee\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kwhorne\\WirementBreezeee\\WirementBreezeeeServiceProvider"
            ],
            "aliases": {
                "WirementBreezeee": "Kwhorne\\WirementBreezeee\\Facades\\WirementBreezeee"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
